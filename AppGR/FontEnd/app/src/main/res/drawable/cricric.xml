<!-- rounded_imageview.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="100dp" /> <!-- Điều chỉnh giá trị radius để điều chỉnh độ cong của viền -->
    <solid android:color="@android:color/transparent" /> <!-- Đặt màu nền của viền -->
    <stroke
        android:width="2dp"
    android:color="@android:color/white" />
</shape>